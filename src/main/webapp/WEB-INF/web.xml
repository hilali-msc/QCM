<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>qcm</display-name>
  <welcome-file-list>
    <welcome-file>/</welcome-file>
  </welcome-file-list>
  <resource-ref>
    <description></description>
    <res-ref-name>jdbc/GestionQCM</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>qcm authentication</web-resource-name>
      <url-pattern>/*</url-pattern>
      <http-method>GET</http-method>
      <http-method>POST</http-method>
    </web-resource-collection>
    <auth-constraint>
      <role-name>1</role-name>
      <role-name>2</role-name>
    </auth-constraint>
    <user-data-constraint>
      <transport-guarantee>NONE</transport-guarantee>
    </user-data-constraint>
  </security-constraint>
  <login-config>
    <auth-method>FORM</auth-method>
    <form-login-config>
      <form-login-page>/authentification.jsp</form-login-page>
      <form-error-page>/authentification.jsp</form-error-page>
    </form-login-config>
  </login-config>
  <servlet>
    <description></description>
    <display-name>StagiairesServlet</display-name>
    <servlet-name>StagiairesServlet</servlet-name>
    <servlet-class>fr.eni.servlets.StagiairesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StagiairesServlet</servlet-name>
    <url-pattern>/stagiaires</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MainServlet</display-name>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>fr.eni.servlets.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AjouterStagiaireServlet</display-name>
    <servlet-name>AjouterStagiaireServlet</servlet-name>
    <servlet-class>fr.eni.servlets.AjouterStagiaireServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjouterStagiaireServlet</servlet-name>
    <url-pattern>/ajouterStagiaire</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SuppressionStagiaireServlet</display-name>
    <servlet-name>SuppressionStagiaireServlet</servlet-name>
    <servlet-class>fr.eni.servlets.SuppressionStagiaireServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SuppressionStagiaireServlet</servlet-name>
    <url-pattern>/suppressionStagiaire</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DeconnexionServlet</display-name>
    <servlet-name>DeconnexionServlet</servlet-name>
    <servlet-class>fr.eni.servlets.DeconnexionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeconnexionServlet</servlet-name>
    <url-pattern>/deconnexion</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ModifierStagiaireServlet</display-name>
    <servlet-name>ModifierStagiaireServlet</servlet-name>
    <servlet-class>fr.eni.servlets.ModifierStagiaireServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ModifierStagiaireServlet</servlet-name>
    <url-pattern>/modifierStagiaire</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TestsServlet</display-name>
    <servlet-name>TestsServlet</servlet-name>
    <servlet-class>fr.eni.servlets.TestsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestsServlet</servlet-name>
    <url-pattern>/tests</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AjouterTestServlet</display-name>
    <servlet-name>AjouterTestServlet</servlet-name>
    <servlet-class>fr.eni.servlets.AjouterTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjouterTestServlet</servlet-name>
    <url-pattern>/ajouterTest</url-pattern>
  </servlet-mapping>
</web-app>