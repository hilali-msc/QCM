		<servlet-class>fr.eni.servlets.GestionStagiaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GestionStagiaire</servlet-name>
		<url-pattern>/GestionStagiaire</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>TestServlet</display-name>
		<servlet-name>TestServlet</servlet-name>
		<servlet-class>fr.eni.servlets.TestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestServlet</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
