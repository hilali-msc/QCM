	<servlet-mapping>
		<servlet-name>ConsultationTestServlet</servlet-name>
		<url-pattern>/ConsultationTestServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ResultatTestServlet</display-name>
